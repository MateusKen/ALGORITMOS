def doorlock1():
	print("	Você precisa de uma SENHA para abrir.")
	RES3 = input()
	if RES3 == str(n1) + str(n2) + str(n3) + str(n4):
		porta()
		sala7()
	else:
		print()
		print("	Senha errada.")
		print()
		sala4()

def doorlock2():
	print("	Você precisa de uma SENHA para abrir.")
	RES5 = input()
	if RES5 == str(N1) + str(N2) + str(N3) + str(N4):
		porta()
		sala9()
	else:
		print()
		print("Senha errada.")
		print()
		sala5()

def doorlock3():
	print("	Você precisa de uma SENHA para abrir.")
	RES4 = input()
	if RES4 == str(n_1) + str(n_2) + str(n_3) + str(n_4):
		porta()
		story3()
	else:
		print()
		print("	Senha errada.")
		print()
		sala5()

def puzzle1():
	print()
	print("	A porta não se abre ela precisa de uma senha para abrir")
	print("SENHA:")
	RES2 = input()
	if RES2 == "PESSOA" or RES2 == "pessoa" or RES2 == "Pessoa":
		porta()
		sala6()
	else:
		print()
		print("	Senha errada.")
		print()
		sala1()

def puzzle2():
	print()
	print("	A porta não se abre ela precisa de uma senha para abrir")
	print("SENHA:")
	RES1 = input()
	if RES1 == "Seis horas" or RES1 == "seis horas" or RES1 == "SEIS HORAS" or RES1 == "6 horas" or RES1 == "6 Horas" or RES1 == "6 HORAS":
		porta()
		sala3()
	else:
		print()
		print("	Senha errada.")
		print()
		sala2()


def story1():
	print()
	print("	'Que sala estranha, parando para pensar, o que eu estou fazendo aqui'")
