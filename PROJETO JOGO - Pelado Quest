def p1():
	print()
	print("O que vai fazer?	")
	print("A. Nada")
	print("B. Quase nada")
	print("C. Absolutamente nada")
	print("D. Deitar e chorar, eu não consigo fazer nada sem as minhas roupas")
	print()
	res0 = input()
	if res0 == "D" or res0 == "d":
		line001()
	if res0 == "A" or res0 == "a" or res0 == "C" or res0 == "c":
	 line01()
	 sala1()
	if res0 == "b"or res0 == "B":
		line1()
		mp1()
		sala1()
	else:
		p1()

def sala1():
	print()
	print("O que vai fazer?")
	print("A. Pegar aquela cueca jogada no canto da sala, afinal, eu não gosto de andar pelado por aí ur dur.")
	print("B. Ir na porta ao Oeste.")
	print("C. Ir na porta ao Leste.")
	print("D. Vasculhar a sala.")
	print("E. Ver o mapa.")
	print()
	res1 = input()
	if res1 == "A"or res1 == "a":
		print("Bem, parece que você ainda não entendeu o conceito do jogo, você perdeu.")
		cueca()
	if res1 == "D" or res1 == "d":
		dica1()
		sala1()
	if res1 == "E" or res1 == "e":
		print()
		mp1()
		sala1()	
	if res1 == "C" or res1 == "c":
		puzzle1()
	if res1 == "B"or res1 == "b":
		porta()
		line02()
		sala2()
	else:
		print()
		print("Resposta inválida.")
		sala1()

